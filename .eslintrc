{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": ["airbnb-base"],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 140 }]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"]
        }
      }
    }
}